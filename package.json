    "mongoose": "^4.11.1",
    "morgan": "^1.6.1",
    "negotiator": "^0.6.1",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "serve-favicon": "~2.3.0",
